<odoo>

    <record id="action_custom_sale_report" model="ir.actions.report">
        <field name="name">Custom Sales Order Report</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">custom_report.custom_sale_report_template</field>
        <field name="report_file">custom_report.custom_sale_report_template</field>
        <field name="print_report_name">'Sales Order - %s' % (object.name)</field>
    </record>

</odoo>
