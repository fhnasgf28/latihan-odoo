<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_gemini_reminder" model="ir.cron">
            <field name="name">Gemini: Kirim Email Motivasi Harian</field>
            <field name="model_id" ref="model_gemini_motivator"/>
