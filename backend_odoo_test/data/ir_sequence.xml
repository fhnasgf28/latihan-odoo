<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_material_code" model="ir.sequence">
            <field name="name">Material Code Sequence</field>
            <field name="code">material.material.code</field>
            <field name="prefix">MAT/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_material_requisition" model="ir.sequence">
            <field name="name">Material Requisition Sequence</field>
            <field name="code">material.requisition</field>
            <field name="prefix">MREQ/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>